enableLocalSpy: "<yellow>LocalSpy wurde <green>aktiviert<yellow>!"
disableLocalSpy: "<yellow>LocalSpy wurde <red>deaktiviert<yellow>!"

####################################################################################################
# Error Messages
####################################################################################################
