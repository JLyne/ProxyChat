####################################################################################################
# Channel Type Messages
####################################################################################################
enableGlobal: "<yellow>Du sprichst nun im <aqua>Global Chat<yellow>!"
enableStaffchat: "<yellow>Du sprichst nun im <green>Team Chat<yellow>!"
