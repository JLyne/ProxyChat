unmute: "<red>%name% <yellow>újra beszélhet!"
mute: "<red>%name% <yellow>némítva lett!"
tempmute: "<red>%name% <yellow>némítva lett eddig -> %muted_until%"

####################################################################################################
# Spy Messages
####################################################################################################
enableSocialSpy: "<yellow>Privát üzenetek láthatósága <green>bekapcsolva<yellow>!"
disableSocialSpy: "<yellow>Privát üzenetek láthatósága <red>kikapcsolva<yellow>!"
