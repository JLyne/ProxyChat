disableLocalSpy: "<yellow>Helyi chatek láthatósága <red>kikapcsolva<yellow>!"

####################################################################################################
# Error Messages
####################################################################################################
notPlayer: "<red>Ezt a parancsot csak játékosok használhatják!"
playerNotFound: "<red>Nem található játékos..."
